{
	"compilerOptions": {
		"target": "es2016",
		"jsx": "react",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"baseUrl": "./",
		"resolveJsonModule": true,
		"paths": {
			"Components/*": ["src/Components/*"],
			"Utils/*": ["src/Utils/*"],
			"Views/*": ["src/Views/*"]
		}
	},
	"include": ["src/**/*"]
}
