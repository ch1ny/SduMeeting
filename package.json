{
  "name": "chirp",
  "version": "0.2.0",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^4.18.5",
    "fluent-ffmpeg": "^2.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-scripts": "5.0.0",
    "redux": "^4.1.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "set BROWSER=none&&react-scripts start",
    "build": "set GENERATE_SOURCEMAP=false&&react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "elect": "set NODE_ENV=development&&electron .",
    "obfuscator": "obfuscator.bat",
    "dist-win": "yarn obfuscator&&electron-packager ./build %npm_package_name% --platform=win32 --arch=x64 --out ./dist --asar --overwrite",
    "win": "yarn build&&yarn dist-win&&extraNodeModules.bat win32 x64"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^16.0.8",
    "electron-packager": "^15.4.0",
    "electron-store": "^8.0.1",
    "javascript-obfuscator": "^3.1.0"
  }
}
